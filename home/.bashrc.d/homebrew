export HOMEBREW_NO_EMOJI=1

if type brew > /dev/null 2>&1; then
  if [ -f $(brew --prefix)/etc/bash_completion ]; then
		. $(brew --prefix)/etc/bash_completion
	fi
fi

if type brew > /dev/null 2>&1; then
  alias ls='gls --color=auto'
  alias cp=gcp
  alias mv=gmv
  alias rm=grm
  alias sed=gsed
  alias sort=gsort
  alias head=ghead
  alias tail=gtail
  alias cat=gcat
  alias tac=gtac
  alias tee=gtee
fi

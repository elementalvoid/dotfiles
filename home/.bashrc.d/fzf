[[ $- = *i* ]] || return

# Setup fzf
# ---------
if [[ ! "$PATH" == */home/mklich/.homesick/repos/fzf/bin* ]]; then
  export PATH="$PATH:/home/mklich/.homesick/repos/fzf/bin"
fi

# Auto-completion
# ---------------
[[ $- == *i* ]] && source "/home/mklich/.homesick/repos/fzf/shell/completion.bash" 2> /dev/null

# Key bindings
# ------------
source "/home/mklich/.homesick/repos/fzf/shell/key-bindings.bash"

